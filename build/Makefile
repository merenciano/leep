# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  leep_config = debug
endif
ifeq ($(config),release)
  leep_config = release
endif
ifeq ($(config),trace)
  leep_config = trace
endif

PROJECTS := leep

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

leep:
ifneq (,$(leep_config))
	@echo "==== Building leep ($(leep_config)) ===="
	@${MAKE} --no-print-directory -C . -f leep.make config=$(leep_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f leep.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo "  trace"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   leep"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"